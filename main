    def calculateFramerate(self):
        #calculate the current framerate and display to the screen
        self.currentTick = pg.time.get_ticks()
        hold = self.currentTick - self.lastTick

        if (hold >= 1000):
            framerate = max(1, int(1000.0 * self.frames / hold))
            pg.display.set_caption(f"running at {framerate} fps.")
            self.lastTick = self.currentTick
            self.frames = -1
            self.frameTime = float (1000.0/ max(1, framerate))
        self.frames += 1

    #Quit pygame
    def quit(self):
        pg.quit()

class Light:

    #Initialise the position and direction of the light
    def __init__(self, position, phi, theta):
        self.position = np.array(position, dtype = np.float32)
        self.theta = theta
        self.phi = phi
        #call function to calculate vectors
        self.recalculateVectors()

    #Calculate forwards, up and right vectors to be used in light direction calculations
    def recalculateVectors(self):
        self.forwards = np.array(
            [   np.cos(np.deg2rad(self.theta)) * np.cos(np.deg2rad(self.phi)),
                np.sin(np.deg2rad(self.theta)) * np.cos(np.deg2rad(self.phi)),
                np.sin(np.deg2rad(self.phi)) ], dtype = np.float32)

        self.right = pyrr.vector3.cross(self.forwards, np.array([0, 0, 1], dtype=np.float32))

        self.up = pyrr.vector3.cross(self.right, self.forwards)

class Plane:

    #Setup class to hold all the necessary data to calculate if a ray hits a plane
    def __init__(self, normal, tangent, bitangent, uMin, uMax, vMin, vMax, center, colour, reflection):
        self.normal = np.array(normal, dtype=np.float32)
        self.tangent = np.array(tangent, dtype=np.float32)
        self.bitangent = np.array(bitangent, dtype=np.float32)
        self.uMin = uMin
        self.uMax = uMax
        self.vMin = vMin
        self.vMax = vMax
        self.center = np.array(center, dtype=np.float32)
        self.colour = np.array(colour, dtype=np.float32)
        self.reflection = reflection

class Sphere:

    #Setup class to hold data on spheres necessary to calculate if a ray hits a sphere
    def __init__(self, center, radius, colour):
        self.center = np.array(center, dtype = np.float32)
        self.radius = radius
        self.colour = np.array(colour, dtype=np.float32)

class RayTrace:

    def __init__(self, width, height):

        #Setup screen width and height
        self.screenWidth = width
        self.screenHeight = height

        #Call functions to create basic and compute shaders and link to necessry shader files
        self.rayTracerShader = self.createComputeShader("shaders/computeShader.txt")
        self.shader = self.createShader("shaders/vertexShader.txt", "shaders/fragmentShader.txt")

        #Call functions to run shader and create image
        glUseProgram(self.shader)
        self.createScreen()
        self.createColourBuffer()
        self.createObjectMemory()

    def createScreen(self):
        #Setup square plane to render image to (x, y, z (coordinates), s, t (texture coordinates)
        self.vertices = np.array(
            (1.0, 1.0, 0.0, 1.0, 1.0,  # top-right
             -1.0, 1.0, 0.0, 0.0, 1.0,  # top-left
             -1.0, -1.0, 0.0, 0.0, 0.0,  # bottom-left
             -1.0, -1.0, 0.0, 0.0, 0.0,  # bottom-left
             1.0, -1.0, 0.0, 1.0, 0.0,  # bottom-right
             1.0, 1.0, 0.0, 1.0, 1.0),  # top-right
            dtype=np.float32)

        self.vertex_count = 6

        #Setup a vertex array to be sent to basic shader and bind to the vertices data stated previously
        self.vao = glGenVertexArrays(1)
        glBindVertexArray(self.vao)
        self.vbo = glGenBuffers(1)
        glBindBuffer(GL_ARRAY_BUFFER, self.vbo)
        glBufferData(GL_ARRAY_BUFFER, self.vertices.nbytes, self.vertices, GL_STATIC_DRAW)

        #Specify how to read through the array to retrieve necessary data
        glEnableVertexAttribArray(0)
        glVertexAttribPointer(0, 3, GL_FLOAT, GL_FALSE, 20, ctypes.c_void_p(0))
        glEnableVertexAttribArray(1)
        glVertexAttribPointer(1, 2, GL_FLOAT, GL_FALSE, 20, ctypes.c_void_p(12))

    def createColourBuffer(self):

        #Generate a texture to send image data to
        self.colourBuffer = glGenTextures(1)
        glActiveTexture(GL_TEXTURE0)
        glBindTexture(GL_TEXTURE_2D, self.colourBuffer)

        #Setup parameters for texture
        glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_WRAP_S, GL_REPEAT)
        glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_WRAP_T, GL_REPEAT)
        glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_MAG_FILTER, GL_NEAREST)
        glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_MIN_FILTER, GL_NEAREST)

        #Specify how to read data from texture
        glTexImage2D(GL_TEXTURE_2D, 0, GL_RGBA32F, self.screenWidth, self.screenHeight, 0, GL_RGBA, GL_FLOAT, None)

    def createObjectMemory(self):

        #To optimise programme and allow higher number of objects, write data to a texture to be unpacked in compute shader
        objectData = []

        # sphere: (cx cy cz r) (r g b -)     (- - - -)     (- - - -)             (- - - -)
        # plane: (cx cy cz tx) (ty tz bx by) (bz nx ny nz) (umin umax vmin vmax) (r g b ref)
        #Create space for up to 1024 planes and spheres
        for object in range(1024):
            for attribute in range(20):
                objectData.append(0.0)
        self.objectData = np.array(objectData, dtype=np.float32)

        #Generate texture to send data to
        self.objectDataTexture = glGenTextures(1)
        glActiveTexture(GL_TEXTURE1)
        glBindTexture(GL_TEXTURE_2D, self.objectDataTexture)

        #Setup parameters for texture
        glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_WRAP_S, GL_REPEAT)
        glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_WRAP_T, GL_REPEAT)
        glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_MAG_FILTER, GL_NEAREST)
        glTexParameteri(GL_TEXTURE_2D, GL_TEXTURE_MIN_FILTER, GL_NEAREST)

        #Specify how to read data from texture
        glTexImage2D(GL_TEXTURE_2D, 0, GL_RGBA32F, 5, 1024, 0, GL_RGBA, GL_FLOAT, bytes(self.objectData))

    def createShader(self, vertexFilepath, fragmentFilepath):

        #Function is input two path files corresponding to basic vertex and fragment shaders
        #Open and read specified shader files
        with open(vertexFilepath, 'r') as f:
            vertex_src = f.readlines()

        with open(fragmentFilepath, 'r') as f:
            fragment_src = f.readlines()

        #Compile files to create shader using OpenGL compileProgram function
        shader = compileProgram(compileShader(vertex_src, GL_VERTEX_SHADER), compileShader(fragment_src, GL_FRAGMENT_SHADER))

        return shader

    def createComputeShader(self, filepath):

        #Function is input path file to ray tracing shader
        #Open and read specified shader file
        with open(filepath, 'r') as f:
            compute_src = f.readlines()

        #Compile file to create compute shader using OpenGL compileProgram function
        shader = compileProgram(compileShader(compute_src, GL_COMPUTE_SHADER))

        return shader

    def prepareSphere(self, i, _sphere):
        # pack sphere data into string of 20 numbers to be loaded into texture and sent to compute shader
        # sphere: (cx cy cz r) (r g b -) (- - - -) (- - - -) (- - - -)

        self.objectData[20 * i] = _sphere.center[0]
        self.objectData[20 * i + 1] = _sphere.center[1]
        self.objectData[20 * i + 2] = _sphere.center[2]

        self.objectData[20 * i + 3] = _sphere.radius

        self.objectData[20 * i + 4] = _sphere.colour[0]
        self.objectData[20 * i + 5] = _sphere.colour[1]
        self.objectData[20 * i + 6] = _sphere.colour[2]

    def preparePlane(self, i, _plane):
        # pack plane data into string of 20 numbers to be loaded into texture and sent to compute shader
        # plane: (cx cy cz tx) (ty tz bx by) (bz nx ny nz) (umin umax vmin vmax) (r g b ref)

        self.objectData[20 * i] = _plane.center[0]
        self.objectData[20 * i + 1] = _plane.center[1]
        self.objectData[20 * i + 2] = _plane.center[2]

        self.objectData[20 * i + 3] = _plane.tangent[0]
        self.objectData[20 * i + 4] = _plane.tangent[1]
        self.objectData[20 * i + 5] = _plane.tangent[2]

        self.objectData[20 * i + 6] = _plane.bitangent[0]
        self.objectData[20 * i + 7] = _plane.bitangent[1]
        self.objectData[20 * i + 8] = _plane.bitangent[2]

        self.objectData[20 * i + 9] = _plane.normal[0]
        self.objectData[20 * i + 10] = _plane.normal[1]
        self.objectData[20 * i + 11] = _plane.normal[2]

        self.objectData[20 * i + 12] = _plane.uMin
        self.objectData[20 * i + 13] = _plane.uMax
        self.objectData[20 * i + 14] = _plane.vMin
        self.objectData[20 * i + 15] = _plane.vMax

        self.objectData[20 * i + 16] = _plane.colour[0]
        self.objectData[20 * i + 17] = _plane.colour[1]
        self.objectData[20 * i + 18] = _plane.colour[2]

        self.objectData[20 * i + 19] = _plane.reflection

    def updateRendering(self, scene):

        #initialise update state
        scene.outDated = False

        #Call compute shader
        glUseProgram(self.rayTracerShader)
        #Setup uniforms for ambient light intensity and light colour
        glUniform3fv(glGetUniformLocation(self.rayTracerShader, "ambient"), 1, np.array([0.1, 0.1, 0.1], dtype = np.float32))
        glUniform3fv(glGetUniformLocation(self.rayTracerShader, "lightCol"), 1, np.array([1, 1, 1], dtype=np.float32))

        #Setup uniform for number of spheres and bring sphere data accross from scene
        glUniform1f(glGetUniformLocation(self.rayTracerShader, "sphereCount"), len(scene.spheres))
        for i, _sphere in enumerate(scene.spheres):
            self.prepareSphere(i, _sphere)

        # Setup uniform for number of planes and bring plane data accross from scene
        glUniform1f(glGetUniformLocation(self.rayTracerShader, "planeCount"), len(scene.planes))
        for i, _plane in enumerate(scene.planes):
            self.preparePlane(i + len(scene.spheres), _plane)

        #Generate texture to hold objext data and set parameters
        glActiveTexture(GL_TEXTURE1)
        glBindTexture(GL_TEXTURE_2D, self.objectDataTexture)
        glTexImage2D(GL_TEXTURE_2D, 0, GL_RGBA32F, 5, 1024, 0, GL_RGBA, GL_FLOAT, bytes(self.objectData))

    def prepareScene(self, scene):

        #Call compute shader
        glUseProgram(self.rayTracerShader)

        #Setup uniforms for light position and direction vectors
        glUniform3fv(glGetUniformLocation(self.rayTracerShader, "viewer.position"), 1, scene.light.position)
        glUniform3fv(glGetUniformLocation(self.rayTracerShader, "viewer.forwards"), 1, scene.light.forwards)
        glUniform3fv(glGetUniformLocation(self.rayTracerShader, "viewer.right"), 1, scene.light.right)
        glUniform3fv(glGetUniformLocation(self.rayTracerShader, "viewer.up"), 1, scene.light.up)

        #Check if rendering is outdated and updtate if necessary
        if scene.outDated:
            self.updateRendering(scene)

        #Generate texture and set parameters
        glActiveTexture(GL_TEXTURE1)
        glBindImageTexture(1, self.objectDataTexture, 0, GL_FALSE, 0, GL_READ_ONLY, GL_RGBA32F)

    def renderScene(self, scene):

        #Call compute shader
        glUseProgram(self.rayTracerShader)

        #Call prepareScene function
        self.prepareScene(scene)

        #Generate texture and set parameters
        glActiveTexture(GL_TEXTURE0)
        glBindImageTexture(0, self.colourBuffer, 0, GL_FALSE, 0, GL_WRITE_ONLY, GL_RGBA32F)

        #Specify how many operations the compute shader should carry out (screen width x screen height)
        glDispatchCompute(self.screenWidth, self.screenHeight, 1)

        #Ensure writing to image has finished before reading
        glMemoryBarrier(GL_SHADER_IMAGE_ACCESS_BARRIER_BIT)
        glBindImageTexture(0, 0, 0, GL_FALSE, 0, GL_WRITE_ONLY, GL_RGBA32F)
        #Call function to draw the scene
        self.drawScreen()

    def drawScreen(self):
        #Switch to standard shader, load in the colour buffer and draw it to the screen
        glUseProgram(self.shader)
        glBindFramebuffer(GL_FRAMEBUFFER, 0)
        glActiveTexture(GL_TEXTURE0)
        glBindTexture(GL_TEXTURE_2D, self.colourBuffer)
        glBindVertexArray(self.vao)
        glDrawArrays(GL_TRIANGLES, 0, self.vertex_count)
        pg.display.flip()

    def destroy(self):
        #Free up any allocated memory
        glUseProgram(self.rayTracerShader)
        glMemoryBarrier(GL_ALL_BARRIER_BITS)
        glDeleteProgram(self.rayTracerShader)
        glDeleteVertexArrays(1, (self.vao,))
        glDeleteBuffers(1, (self.vbo,))
        glDeleteTextures(1, (self.colourBuffer,))
        glDeleteProgram(self.shader)

