import pygame as pg
from OpenGL.GL import *
from OpenGL.GL.shaders import compileProgram,compileShader
import numpy as np
import pyrr

class Program:

    def __init__(self):

        #Initialise pygame and set the screen width and height
        pg.init()
        self.screenWidth = 1000
        self.screenHeight = 600

        #Request python version 4.3 (required for compute shaders) and setup core profile
        pg.display.gl_set_attribute(pg.GL_CONTEXT_MAJOR_VERSION, 4)
        pg.display.gl_set_attribute(pg.GL_CONTEXT_MINOR_VERSION, 3)
        pg.display.gl_set_attribute(pg.GL_CONTEXT_PROFILE_MASK, pg.GL_CONTEXT_PROFILE_CORE)

        #Create window of screen width and height, specifying the use of openGL with double buffer rendering
        pg.display.set_mode((self.screenWidth, self.screenHeight), pg.OPENGL | pg.DOUBLEBUF)

        #Call the ray tracing function
        self.rayTracer = RayTrace(self.screenWidth, self.screenHeight)

        #Set the opening scene
        self.scene = Scene1()

        #Get game ticks and initialise variables to calculate framerate
        self.lastTick = pg.time.get_ticks()
        self.currentTick = 0
        self.frames = 0
        self.frameTime = 0

        #Call the main loop to run the programme
        self.programLoop()

    #Run until programme is wuit calling the rendering functions and checking for new input
    def programLoop(self):

        running = True
        #Loop until programme ends
        while (running):
            #Check if exit keys are pressed
            for event in pg.event.get():
                if (event.type == pg.QUIT):
                    running = False
                if (event.type == pg.KEYDOWN):
                    if (event.key == pg.K_ESCAPE):
                        running = False

            #Change position and scene depending on keys pressed
            self.keyChange()
            #Render the scene
            self.rayTracer.renderScene(self.scene)

            #Calculate framerate
            self.calculateFramerate()
        self.quit()

    def keyChange(self):

        keys = pg.key.get_pressed()
        if keys[pg.K_1]:
            self.scene = Scene1()
        if keys[pg.K_2]:
